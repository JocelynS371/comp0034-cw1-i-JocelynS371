import dash
import dash_core_components as dcc
import dash_html_components as html

app = dash.Dash()

app.layout = html.Div([
    dcc.Tabs(id="tabs", value='tab-1', children=[
        dcc.Tab(label='Tab 1', value='tab-1', children=[
            html.Div(children='This is the content of Tab 1')
        ]),
        dcc.Tab(label='Tab 2', value='tab-2', children=[
            html.Div(children='This is the content of Tab 2')
        ]),
        dcc.Tab(label='Tab 3', value='tab-3', children=[
            html.Div(children='This is the content of Tab 3')
        ]),
    ])
])

if __name__ == '__main__':
    app.run_server(debug=True)





